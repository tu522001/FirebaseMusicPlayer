<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.firebasemusicplayer.view.user.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_screenFragment"
            app:destination="@id/screenFragment" />
        <action
            android:id="@+id/action_homeFragment_to_facebookFragment3"
            app:destination="@id/facebookFragment3" />
        <action
            android:id="@+id/action_homeFragment_to_adminFragment"
            app:destination="@id/adminFragment" />
        <action
            android:id="@+id/action_homeFragment_to_adminFragment2"
            app:destination="@id/adminFragment" />
    </fragment>
    <fragment
        android:id="@+id/screenFragment"
        android:name="com.example.firebasemusicplayer.view.user.home.ScreenFragment"
        android:label="fragment_screen"
        tools:layout="@layout/fragment_screen" >
        <action
            android:id="@+id/action_screenFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.firebasemusicplayer.view.user.home.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.example.firebasemusicplayer.view.login.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >

        <action
            android:id="@+id/action_signInFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />

        <action
            android:id="@+id/action_signInFragment_to_facebookFragment3"
            app:destination="@id/facebookFragment3" />
        <action
            android:id="@+id/action_signInFragment_to_adminFragment"
            app:destination="@id/adminFragment" />
        <action
            android:id="@+id/action_signInFragment_to_adminFragment2"
            app:destination="@id/adminFragment" />
        <action
            android:id="@+id/action_signInFragment_to_adminFragment3"
            app:destination="@id/adminFragment" />

    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.example.firebasemusicplayer.view.login.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/facebookFragment3"
        android:name="com.example.firebasemusicplayer.view.login.FacebookFragment"
        android:label="fragment_facebook"
        tools:layout="@layout/fragment_facebook" >

        <action
            android:id="@+id/action_facebookFragment3_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_facebookFragment3_to_termsOfServiceFragment"
            app:destination="@id/termsOfServiceFragment" />
        <action
            android:id="@+id/action_facebookFragment3_to_helpFragment"
            app:destination="@id/helpFragment" />
        <action
            android:id="@+id/action_facebookFragment3_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_facebookFragment3_to_securityFragment"
            app:destination="@id/securityFragment" />
        <action
            android:id="@+id/action_facebookFragment3_to_versionFragment"
            app:destination="@id/versionFragment" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_admin"
        android:id="@+id/adminFragment"
        android:name="com.example.firebasemusicplayer.view.admin.AdminFragment"
        android:label="AdminFragment" >
        <action
            android:id="@+id/action_adminFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_adminFragment_to_facebookFragment3"
            app:destination="@id/facebookFragment3" />
        <action
            android:id="@+id/action_adminFragment_to_termsOfServiceFragment"
            app:destination="@id/termsOfServiceFragment" />
        <action
            android:id="@+id/action_adminFragment_to_helpFragment"
            app:destination="@id/helpFragment" />
        <action
            android:id="@+id/action_adminFragment_to_securityFragment"
            app:destination="@id/securityFragment" />
        <action
            android:id="@+id/action_adminFragment_to_versionFragment"
            app:destination="@id/versionFragment" />
        <action
            android:id="@+id/action_adminFragment_to_signInFragment2"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_adminFragment_to_signInFragment3"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_adminFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_adminFragment_to_addSongFragment"
            app:destination="@id/addSongFragment" />
        <action
            android:id="@+id/action_adminFragment_to_outputFileExcelFragment"
            app:destination="@id/outputFileExcelFragment" />
        <action
            android:id="@+id/action_adminFragment_to_homeFragment2"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_adminFragment_to_homeFragment3"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/termsOfServiceFragment"
        android:name="com.example.firebasemusicplayer.view.user.setting.TermsOfServiceFragment"
        android:label="fragment_terms_of_service"
        tools:layout="@layout/fragment_terms_of_service" >
        <action
            android:id="@+id/action_termsOfServiceFragment_to_facebookFragment3"
            app:destination="@id/facebookFragment3" />
    </fragment>
    <fragment
        android:id="@+id/helpFragment"
        android:name="com.example.firebasemusicplayer.view.user.setting.HelpFragment"
        android:label="fragment_help"
        tools:layout="@layout/fragment_help" >
        <action
            android:id="@+id/action_helpFragment_to_facebookFragment3"
            app:destination="@id/facebookFragment3" />
    </fragment>
    <fragment
        android:id="@+id/securityFragment"
        android:name="com.example.firebasemusicplayer.view.user.setting.SecurityFragment"
        android:label="fragment_security"
        tools:layout="@layout/fragment_security" >
        <action
            android:id="@+id/action_securityFragment_to_facebookFragment3"
            app:destination="@id/facebookFragment3" />
    </fragment>
    <fragment
        android:id="@+id/versionFragment"
        android:name="com.example.firebasemusicplayer.view.user.setting.VersionFragment"
        android:label="fragment_version"
        tools:layout="@layout/fragment_version" >
        <action
            android:id="@+id/action_versionFragment_to_facebookFragment3"
            app:destination="@id/facebookFragment3" />
    </fragment>
    <fragment
        android:id="@+id/outputFileExcelFragment"
        android:name="com.example.firebasemusicplayer.view.admin.OutputFileExcelFragment"
        android:label="fragment_output_file_excel"
        tools:layout="@layout/fragment_output_file_excel" >
        <action
            android:id="@+id/action_outputFileExcelFragment_to_adminFragment"
            app:destination="@id/adminFragment" />
    </fragment>
    <fragment
        android:id="@+id/addSongFragment"
        android:name="com.example.firebasemusicplayer.view.admin.AddSongFragment"
        android:label="fragment_add_song"
        tools:layout="@layout/fragment_add_song" >
        <action
            android:id="@+id/action_addSongFragment_to_adminFragment"
            app:destination="@id/adminFragment" />
    </fragment>
</navigation>